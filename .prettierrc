{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 100,
  "trailingComma": "es5",

  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^[a-z]", "^@", "^\\.\\./", "^\\./", ".scss$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "jsx"]
}
